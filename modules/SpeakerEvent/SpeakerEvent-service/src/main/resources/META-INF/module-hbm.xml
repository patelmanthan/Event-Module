<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="SpeakerEvent.model.Speaker" />
	<import class="SpeakerEvent.model.Speaker_Event" />
	<class name="SpeakerEvent.model.impl.SpeakerImpl" table="FOO_Speaker">
		<id name="speakerId" type="long">
			<generator class="increment" />
		</id>
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="firstName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="SpeakerEvent.model.impl.Speaker_EventImpl" table="FOO_Speaker_Event">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property name="speakerId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="eventId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>